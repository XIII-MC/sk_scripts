command /§zm:
   trigger:
      send "&cThe server you tried to join is offline or the game alerdy started!" to player
      play sound "block.anvil.land" with volume 10 and pitch 1 at player for player


command /se [<text>] [<text>]:
    permission: op
    trigger:
        if arg 1 is set:
            if arg 2 is set:
                make server execute command "/mv %arg 1% %arg 2%"
                send "&aSucess! Server &e%arg 2% &ahas been &e%arg 1%&a!"
                if arg 1 is "unload":
                    set {s%arg 2%} to "&cOffline"
                    play sound "block.anvil.break" with volume 10 and pitch 1 at player for player
                if arg 1 is "load":
                    set {s%arg 2%} to "&aOnline"
                    set {game.%arg 2%} to false

command /set [<text>] [<number>]:
   permission: op
   trigger:
      if arg 1 is set:
         if arg 2 is set:
            set {%arg 1%} to arg 2
            send "&aSucess! Setted &e%arg 1% &ato &e%arg 2%&a!" to player


command /forcestop [<world>]:
   permission: ADMIN
   trigger:
      if arg 1 is set:
         set {game.%arg 1%} to false
         send "&aSucess!"
         loop all players in arg 1:
            send "&cYour game has been ended by &d%player%&c! &cYou will get automaticly teleported back to lobby in a few seconds..." to loop-player
            make server execute command "/mvtp %loop-player% ul_world"
            make server execute command "/reset %arg 1%"

on world change:
   if player's world is "ul_world":
      if {mod.%player%} is true:
         stop
      set player's gamemode to adventure

every 2 second:
   loop all players in "ul_world":
      set {y.%loop-player%} to y location of loop-player
      if {y.%loop-player%} < 10:
         make server execute command "/mvspawn %loop-player%"

on damage:
   if victim's world is "ul_world":
      cancel event

on block place:
   if player's world is "ul_world":
      cancel event

on drop:
   if player's world is "ul_world":
      cancel event

on block break:
   if player's world is "ul_world":
      cancel event

every 5 second:
   if {br} = 0:
      if {br.down} is true:
         stop
      make server execute command "/se unload br"
      send "&6&lSERVICES &7>> &cServer &6BR &cwas shutted down ! No players were on." to all players where [input has permission "BUILDER"]
      set {br.down} to true

on script load:
   make server execute command "/se load br1"
   make server execute command "/se unload br2"
   make server execute command "/se unload br3"
   make server execute command "/se unload br4"
   make server execute command "/se unload br5"
   make server execute command "/se unload br6"
   make server execute command "/se unload br7"
   make server execute command "/se unload br8"
   make server execute command "/se unload br9"
   make server execute command "/se unload br10"
   make server execute command "/se unload backup"
   make server execute command "/se unload ul_br"
   make server execute command "/se unload br"

command /reset [<text>]:
   permission: ADMIN
   trigger:
      if arg 1 is set:
         make server execute command "/mv delete %arg 1%"
         make server execute command "/mvconfirm"
         wait 5 seconds
         make server execute command "/mv clone ul_br %arg 1%"
         wait 5 seconds
         make server execute command "/se load %arg 1%"
         set {%arg 1%} to 0
         set {game.%arg 1%} to false
         set {start.%arg 1%} to false
         set {win.%arg 1%} to false
         set {start.%arg 1%} to false
         send "&aSucess!"


command /bug:
   permission: ADMIN
   trigger:
      if {bug} is not set:
         set {bug} to false
      if {bug} is false:
         set {bug} to true
         broadcast "&b&lFish&e&lRoyale&6Services &cwhere shutted down ! This was activiated by &d%player%&c. It can be cause of lag!"
         play sound "block.anvil.land" with volume 10 and pitch 1 at player for player
         stop
      if {bug} is true:
         set {bug} to false
         broadcast "&b&lFish&e&lRoyale&6Services &aare back on !"

