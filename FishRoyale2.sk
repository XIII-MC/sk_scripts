command /br1:
	permission: op
	trigger:
		clear {_all.players::*}

		set {_all.players::*} to all players where [world of player input is {wbr1}]
		if {start.br1} is true:
			stop
		set {start.br1} to true
		
		send "" to {_all.players::*}
		send "&e&lA Battle Royale is starting soon." to {_all.players::*}
		send "&e&lTo join it type &b/join br1" to {_all.players::*}
		send "" to {_all.players::*}
			
		loop 40 times:
			if loop-number is 1 or 11 or 38:
				send "&aGame is starting in %41-loop-number% &aseconds" to {_all.players::*}
				loop {_all.players::*}:
					play sound "entity.experience_orb.pickup" with volume 10 and pitch 1 at loop-value-2 for loop-value-2
			wait 1 second
			
		if {br1} < 4:
			send "&cNot enough players to start the game." to {_all.players::*}
			set {start.br1} to false
			stop
			
		send "&7&m-----------------------" to {_all.players::*}
		send "          &6&lGame started !" to {_all.players::*}
		send "          &d&lGood luck !" to {_all.players::*}
		send "&7&m-----------------------" to {_all.players::*}
		
		loop {_all.players::*}:
			loop blocks in radius 5 around loop-value:
				if loop-block is barrier:
					set loop-block to air
			play sound "entity.enderdragon.ambient" with volume 10 and pitch 1 at loop-value for loop-value
			wait 2 tick
			
		set gamemode of {_all.players::*} to survival 
		
		heal {_all.players::*} by 20 heart 
		make server execute command "/effect clear %{_all.players::*}%"

		set {game.br1} to true
		set {sbr1} to "&6Playing"
		set {start.br1} to true
		clear {_all.players::*}
		
		if {fss.br1} is true:
			set {fss.br1} to false
			remove 4 from {br1}

