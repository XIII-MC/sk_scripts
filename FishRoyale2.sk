         send "&b&lFish&e&lRoyale&6Services &cAre currently down! For help contact a staff member."
         play sound "block.anvil.land" with volume 10 and pitch 1 at player for player
         stop
      if player's world is not "ul_world":
         send "&cYou can't switch games!"
         play sound "block.anvil.land" with volume 10 and pitch 1 at player for player
         stop
      if arg 1 is set:
         if {game.%arg 1%} is true:
            send "&cThis game alerdy started!"
            play sound "block.anvil.land" with volume 10 and pitch 1 at player for player
            stop
         if {game.%arg 1%} is false:
            send "&7Sending you to &e%arg 1%"
            make server execute command "/mvtp %player% %arg 1%"
      if arg 1 is not set:
         send "&cPlease use: &f/join &6SERVER_ID"
         play sound "block.anvil.land" with volume 10 and pitch 1 at player for player
         stop

