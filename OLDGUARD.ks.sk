#--------------------------------------------------------

on any movement:
  if {legtifly.%player%} is true:
    stop
  set {_loc} to player's location
  if player's chestplate is an elytra:
    stop
  if {%player's uuid%.GUARD.check.epteleport} is true:
    stop
  if {%player's uuid%.GUARD.check.teleport} is true:
    stop
  else if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.check.nochecks} is set:
      wait 1 second
      delete {%player's uuid%.GUARD.check.nochecks}
      stop
    if {%player's uuid%.GUARD.check.bypass} is not set:
      if {%player's uuid%.GUARD.check.cheat.cooldown} is not set:
        set {%player's uuid%.GUARD.check.cheat.cooldown} to true
        if block under player is not air or water:
          set {%player's uuid%.GUARD.check.tp.loc} to {_loc}
        #NoFall
        {GUARD.check.NoFall} is true:
          if player's flight mode is false:
            if {%player's uuid%.GUARD.check.jump} is not set:
              if block under player is air:
                set {_yloc} to player's y coordinate
                set {_health} to player's health
                wait 0.5 seconds
                set {_yloc1} to player's y coordinate
                if player is on ground:
                  block under player is not slime block
                  set {_dis} to {_yloc} - {_yloc1}
                  block under player is not air
                  block under player is ladder:
                    stop
                  loop all blocks in radius 2 around player:
                    if loop-block is cobweb:
                      stop
                    if loop-block is water:
                      stop
                    if loop-block is vine:
                      stop
                    if loop-block is ladder:
                      stop
                  {%player's uuid%.GUARD.check.nofall} is not set
                  if {_dis} is greater than or equal to 5.5:
                    if player's health = {_health}-0.5:
                      set {_dmg} to floor({_dis} / 1)
                      add 1 to {GUARD.%player%.total.vl}
                      detect(player, "Nofall", "2C")
                      if {@Kick} is true:
                        make server execute command "/guard kick %player%"
                        stop
                      if {@Ban} is true:
                        make server execute command "/guard ban %player% 7 day"
                      set {%player's uuid%.GUARD.check.nofall} to true
                      wait 1 second
                      delete {%player's uuid%.GUARD.check.nofall}
                      #damage the player by {_dmg} heart
        #Spider
        {GUARD.check.Spider} is true
        if block under player is air:
          if block 2 under player is air:
            if block 3 under player is air:
              set {_y} to player's y coordinate
              wait 1 tick
              block under player is not ladder
              block at player is not water
              if {%player's uuid%.GUARD.check.jump} is not set:
                if difference between {_y} and player's y coordinate is greater than 0.5:
                  if difference between {_y} and player's y coordinate is less than 1:
                    if distance between player and player's targeted block is less than 1.25:
                      add 1 to {GUARD.%player%.total.vl}
                      detect(player, "Spider", "1A")
                      if {GUARD.%player%.vl} >= 10:
                        if {@Kick} is true:
                          make server execute command "/guard kick %player%"
                          stop
                        if {@Ban} is true:
                          make server execute command "/guard ban %player% 7 day"
                  else:
                    if distance between player and player's targeted block is less than 1.25:
                      {GUARD.check.Speed} is true
                      add 1 to {GUARD.%player%.total.vl}
                      detect(player, "Spider", "2A")
                      if {GUARD.%player%.vl} >= 5:
                        if {@Kick} is true:
                          make server execute command "/guard kick %player%"
                          stop
                        if {@Ban} is true:
                          make server execute command "/guard ban %player% 7 day"
              else:
                if difference between {_y} and player's y coordinate is greater than 0.5:
                  if distance between player and player's targeted block is less than 1.25:
                    add 1 to {GUARD.%player%.total.vl}
                    detect(player, "Spider", "3A")
                    if {GUARD.%player%.vl} >= 5:
                      if {@Kick} is true:
                        make server execute command "/guard kick %player%"
                        stop
                      if {@Ban} is true:
                        make server execute command "/guard ban %player% 7 day"
        #Flight
        if player's flight mode is false:
          if block under player is air:
            if player's chestplate is not an elytra:
              if player doesn't have jump boost:
                set {_y} to player's y coordinate
                wait 10 ticks
                if block under player is air:
                  set {_y} to player's y coordinate - {_y}
                  if {_y} >= 2:
                    wait 1 tick
                    if {%player's uuid%.GUARD.check.fix.slime} is not set:
                      set {_find} to player's location
                      set {_count} to 255 - y coordinate of player
                      loop 255 times:
                        if block at {_find} is slime block:
                          stop
                          set {%player's uuid%.GUARD.check.slime} to true
                          wait 10 second
                          delete {%player's uuid%.GUARD.check.slime}
                        else:
                          remove 1 from y coordinate of {_find}
                      if {%player's uuid%.GUARD.check.slime} is not set:
                        if block under player is air:
                          if {%player's uuid%.GUARD.check.slime} is not set:
                            {GUARD.check.Flight} is true
                            block under player is not water or ladder
                            add 1 to {GUARD.%player%.total.vl}
                            detect(player, "Fly", "3C")
                            if {GUARD.%player%.vl} >= 3:
                              if {@Kick} is true:
                                make server execute command "/guard kick %player%"
                                stop
                              if {@Ban} is true:
                                make server execute command "/guard ban %player% 7 day"
                            set {%player's uuid%::GUARD.check.flight.status} to true
                            wait 5 second
                            delete {%player's uuid%::GUARD.check.flight.status}
                  else if {_y} = 0:
                    if block under player is air:
                      set {_y} to y coordinate of player - y coordinate of {%player's uuid%.GUARD.check.tp.loc}
                      if {_y} = 0:
                        set {_b} to block under player
                        set {_b1} to block under {_b}
                        if {_b} is air:
                          if {_b1} is air:
                            if player is not on ground:
                              {GUARD.check.Float} is true
                              if block under player is slab:
                                stop
                              remove 1 from y coordinate of {%player's uuid%.GUARD.check.tp.loc}
                              add 1 to {GUARD.%player%.total.vl}
                              detect(player, "Fly", "2C")
                              if {@Kick} is true:
                                make server execute command "/guard kick %player%"
                                stop
                              if {@Ban} is true:
                                make server execute command "/guard ban %player% 7 day"
          else:
            #HOVER | FLIGHT | SPEED | CRITICALS
            if {%player's uuid%.GUARD.check.nochecks} is true:
              stop
              wait 1 second
              delete {%player's uuid%.GUARD.check.nochecks}
            if {%player's uuid%.GUARD.check.teleport} is true:
              stop
            if {%player's uuid%.GUARD.check.epteleport} is true:
              stop
            else if {%player's uuid%.GUARD.check.jump} is true:
              stop
            if player is riding:
              stop
            else:
              if {%player's uuid%.GUARD.check.falldmg} is not set:
                if player's chestplate is not an elytra:
                  if player is not on ground:
                    if {%player's uuid%.GUARD.check.jump} is not set:
                      set {_loc1} to player's location
                      set {_ycheck} to player's y coordinate
                      set {_123.loc} to player's location
                      wait 10 ticks
                      set {_ycheck1} to player's y coordinate
                      if {_ycheck1} >= {_ycheck}:
                        if player is not on ground:
                          set {_loop} to 255 -  y coordinate of player
                          set y coordinate of {_123.loc} to 255
                          loop {_loop} times:
                            if block at {_123.loc} is slime block:
                              stop
                              set {%player's uuid%.GUARD.check.slime} to true
                              wait 10 second
                              delete {%player's uuid%.GUARD.check.slime}
                            if block at {_123.loc} is air:
                              remove 1 from y coordinate of {_123.loc}
                        if {_loc1} = {_123.loc}:
                          if player is not on ground:
                            wait 3 ticks
                            if {%player's uuid%.GUARD.check.slime} is not set:
                              if {%player's uuid%.GUARD.check.damage} is not set:
                                if {%player's uuid%.GUARD.check.jump} is not set:
                                  wait 2 ticks
                                  if {%player's uuid%.GUARD.check.fix.slime} is not set:
                                    set {_find} to player's location
