on any movement:
  set {_loc} to player's location
  if player's chestplate is an elytra:
    stop
  if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.bypass} is not set:
      player's flight mode is false
      if block under player is not air or water:
        set {%player's uuid%.GUARD.tp.loc} to {_loc}
      if {%player's uuid%.GUARD.flight.delay} is not set:
        set {%player's uuid%.GUARD.flight.delay} to true
        set {_y} to player's y coordinate
        wait 10 ticks
        set {_y} to player's y coordinate - {_y}
        set {_y} to floor({_y})
        if {%player's uuid%.GUARD.slime} is not set:
          if {_y} > 5:
            {GUARD.Flight} is true
            add 1 to {GUARD.%player%.total.vl}
            detect(player, "Fly", "3D")
          if {_y} = 0:
            {GUARD.Float} is true
            {%player's uuid%.GUARD.jump} is not set
            block under player is air
            player is not on ground
            add 1 to {GUARD.%player%.total.vl}
            detect(player, "Fly", "2D")
          if player's y coordinate is y coordinate of {_loc}:
            {GUARD.Float} is true
            {%player's uuid%.GUARD.jump} is not set
            block under player is air
            player is not on ground
            add 1 to {GUARD.%player%.total.vl}
            detect(player, "Fly", "1D")
        loop {GUARD.Delay} times:
          wait 0.21 second
        delete {%player's uuid%.GUARD.flight.delay}

on any movement:
  {GUARD.check.Step} is true
  if {legitfly.%player%} is true:
    stop
  set {_loc} to player's location
  set {_y} to y coordinate of {_loc}
  wait 2 ticks
  set {_y} to player's y coordinate - {_y}
  player doesn't have jump boost
  block under player is not carpet or water
  if {_y} is greater than 1.25:
    wait 1 tick
    player's chestplate is not an elytra
    {%player's uuid%.GUARD.check.nochecks} is not set
    {%player's uuid%.GUARD.check.epteleport} is not set
    {%player's uuid%.GUARD.check.teleport} is not set
    player's gamemode is not creative or spectator
    {%player's uuid%.GUARD.check.bypass} is not set
    player's flight mode is false
    player is not riding
    player is not on ground
    if block under player is not air:
      if {%player's uuid%.GUARD.check.step.player} is not set:
        #OLD VL COUNTER
        add 1 to {GUARD.%player%.total.vl}
        detect(player, "Fly", "1A")
        if {GUARD.%player%.vl} >= 5:
          if {@Kick} is true:
            make server execute command "/guard kick %player%"
            stop
          if {@Ban} is true:
            make server execute command "/guard ban %player% 7 day"
    else:
      {GUARD.check.FLY} is true
      if {legitfly.%player%} is true:
        stop
      #OLD VL COUNTER
      add 1 to {GUARD.%player%.total.vl}
      detect(player, "Fly", "2A")
      if {@setback} is true:
        teleport player to {GUARD.%player%.setback}
      if {GUARD.%player%.vl} >= 5:
        if {@Kick} is true:
          make server execute command "/guard kick %player%"
          stop
        if {@Ban} is true:
          make server execute command "/guard ban %player% 7 day"

on any movement:
  if player's chestplate is an elytra:
    stop
  if {legitfly.%player%} is true:
