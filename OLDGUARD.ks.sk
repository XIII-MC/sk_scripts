on eating:
  if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.bypass} is not set:
      {GUARD.NoSlow} is true
      player's flight mode is false
      if player has speed:
        stop
        add 1 to {GUARD.%player%.total.vl}
        detect(player, "NoSlow", "1A")
        if {GUARD.%player%.vl} >= 3:
          if {@Kick} is true:
            make server execute command "/guard kick %player%"
            stop
          if {@Ban} is true:
            make server execute command "/guard ban %player% 7 day"
        wait 5 seconds
        delete {%player's uuid%.GUARD.cancel.eat}
    if {%player's uuid%.GUARD.cancel.eat} is set:
      {%player's uuid%.GUARD.bypass} is not set
      if {%player's uuid%.GUARD.reach} is set:
        set {%player's uuid%.GUARD.cancel.eat} to true
        add 1 to {GUARD.%player%.total.vl}
        detect(player, "KillAura", "1B")
        if {GUARD.%player%.vl} >= 3:
          if {@Kick} is true:
            make server execute command "/guard kick %player%"
            stop
          if {@Ban} is true:
            make server execute command "/guard ban %player% 7 day"
        wait 5 seconds
        delete {%player's uuid%.GUARD.cancel.eat}
    {GUARD.NoSlow} is true
    set {_loc} to player's location
    set y coordinate of {_loc} to 0
    wait 1 tick
    set {_loc1} to player's location
    set y coordinate of {_loc1} to 0
    set {_loc} to distance between {_loc1} and {_loc}
    if {_loc} > 0.20:
      set {%player's uuid%.GUARD.cancel.eat} to true
      add 1 to {GUARD.%player%.total.vl}
      detect(player, "NoSlow", "2A")
      if {GUARD.%player%.vl} >= 3:
        if {@Kick} is true:
          make server execute command "/guard kick %player%"
          stop
        if {@Ban} is true:
          make server execute command "/guard ban %player% 7 day"
      wait 5 seconds
      delete {%player's uuid%.GUARD.cancel.eat}

