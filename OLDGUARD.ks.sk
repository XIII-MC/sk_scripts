                                            if {@Kick} is true:
                                              make server execute command "/guard kick %player%"
                                              stop
                                            if {@Ban} is true:
                                              make server execute command "/guard ban %player% 7 day"
                                        else:
                                          {GUARD.check.Criticals} is true
                                          add 1 to {GUARD.%player%.total.vl}
                                          detect(player, "Criticals", "1A")
                                          if {@Kick} is true:
                                            make server execute command "/guard kick %player%"
                                            stop
                                          if {@Ban} is true:
                                            make server execute command "/guard ban %player% 7 day"
                              else:
                                {GUARD.check.Criticals} is true
                                add 1 to {GUARD.%player%.total.vl}
                                detect(player, "Criticals", "2A")
                                if {@Kick} is true:
                                  make server execute command "/guard kick %player%"
                                  stop
                                if {@Ban} is true:
                                  make server execute command "/guard ban %player% 7 day"
        #Speed
        if player's flight mode is false:
          if player doesn't have speed or jump boost:
            if {%player's uuid%.GUARD.check.nochecks} is not set:
              if {%player's uuid%.GUARD.check.teleport} is true:
                stop
              else if {%player's uuid%.GUARD.check.epteleport} is true:
                stop
              else:
                set {_x} to player's x coordinate
                set {_z} to player's z coordinate
                set {_bap} to block above player's head
                wait 5 ticks
                set {_bap2} to block above player's head
                set {_x1} to player's x coordinate
                set {_z1} to player's z coordinate
                set {_x} to difference between {_x1} and {_x}
                set {_z} to difference between {_z1} and {_z}
                replace all "-" with "" in {_x}
                replace all "-" with "" in {_z}
                if {_x} > {_z}:
                  set {_speed} to {_x}
                if {_z} > {_x}:
                  set {_speed} to {_z}
                if {_x} = {_z}:
                  set {_speed} to {_x}
                player's walking speed is between 0 and 0.3
                if {_speed} is greater than 3:
                  if {%player's uuid%.GUARD.check.teleport} is true:
                    stop
                  else if {%player's uuid%.GUARD.check.epteleport} is true:
                    stop
                  set {%player's uuid%.GUARD.check.cheat.speed} to {_speed} * 4
                  if player is not riding:
                    if {%player's uuid%.GUARD.check.jump.bhop} is not set:
                      if player is not flying:
                        if block under player is not air:
                          if block 2 under player is not air:
                            {GUARD.check.Speed} is true
                            add 1 to {GUARD.%player%.total.vl}
                            detect(player, "Speed", "1A")
                            if {GUARD.%player%.vl} >= 20:
                              if {@Kick} is true:
                                make server execute command "/guard kick %player%"
                                stop
                              if {@Ban} is true:
                                make server execute command "/guard ban %player% 7 day" 
                          else:
                            {GUARD.check.Speed} is true
                            add 1 to {GUARD.%player%.total.vl}
                            detect(player, "Speed", "2A")
                            if {GUARD.%player%.vl} >= 20:
                              if {@Kick} is true:
                                make server execute command "/guard kick %player%"
                                stop
                              if {@Ban} is true:
                                make server execute command "/guard ban %player% 7 day"      
                      else:
                        {GUARD.check.Speed} is true
                        add 1 to {GUARD.%player%.total.vl}
                        detect(player, "Speed", "3A")
                        if {GUARD.%player%.vl} >= 20:
                          if {@Kick} is true:
                            make server execute command "/guard kick %player%"
                            stop
                          if {@Ban} is true:
                            make server execute command "/guard ban %player% 7 day"                       
                    else:
                      {GUARD.check.Bhop} is true
                      add 1 to {GUARD.%player%.total.vl}
                      detect(player, "Bhop", "1A")
                      if {GUARD.%player%.vl} >= 25:
                        if {@Kick} is true:
                          make server execute command "/guard kick %player%"
                          stop
                        if {@Ban} is true:
                          make server execute command "/guard ban %player% 7 day"      

on jump:
  if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.bypass} is not set:
      if difference between {%player's uuid%.GUARD.last.jump} and now is less than or equal to 1 seconds:
        set {%player's uuid%.GUARD.jump.bhop} to true
        wait 10 ticks
        delete {%player's uuid%.GUARD.jump.bhop}
      set {%player's uuid%.GUARD.last.jump} to now
      set {%player's uuid%.GUARD.jump} to true
      wait 10 ticks
      delete {%player's uuid%.GUARD.jump}

on teleport:
  if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.bypass} is not set:
      set {%player's uuid%.GUARD.teleport} to true
      wait 1 second
      delete {%player's uuid%.GUARD.teleport}

on right click:
  if player's gamemode is not creative or spectator:
    if {%player's uuid%.GUARD.bypass} is not set:
      if player's held item is an enderpearl:
