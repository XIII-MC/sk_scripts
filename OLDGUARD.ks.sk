#             A N T I   S C A F F O L D
#
#--------------------------------------------------------

on block place:
  if {GUARD.check.SCAFFOLD} is false:
    stop
  if {%player's uuid%.GUARD.teleport} is true:
    stop
  else:
    set {_b} to block under player
    set {_b} to block under {_b}
    if {_b} is not air:
      stop
    else:
      if block under event-block is not air:
        stop
      else:
        if player's gamemode is not creative or spectator:
          if {%player's uuid%.GUARD.bypass} is not set:
            if y coordinate of event-block < y coordinate of player:
              if player's pitch is between 9 and -90:
                if {GUARD.delay.scaffold.%player%} is true:
                  stop
                set {GUARD.delay.scaffold.%player%} to true
                if {@setback} is true:
                  cancel event
                detect(player, "Scaffold", "1A")
                if {@Kick} is true:
                  make server execute command "/guard kick %PLAYER%"
                  stop
                if {@Ban} is true:
                  make server execute command "/guard ban %PLAYER% 7 day"
              else:
                set {%player's uuid%.GUARD.old.pitch.time} to {%player's uuid%.GUARD.new.pitch.time}
                set {%player's uuid%.GUARD.old.pitch} to {%player's uuid%.GUARD.new.pitch}
                set {%player's uuid%.GUARD.new.pitch} to player's pitch
                set {%player's uuid%.GUARD.new.pitch.time} to now
                set {_dif.time} to difference between {%player's uuid%.GUARD.old.pitch.time} and {%player's uuid%.GUARD.new.pitch.time}
                set {_dif.pitch} to difference between {%player's uuid%.GUARD.old.pitch} and {%player's uuid%.GUARD.new.pitch}
                if {_dif.time} <= 0.3 seconds:
                  if {_dif.pitch} <= 2.5:
                    add 1 to {GUARD.%player%.total.vl}
                    if {@setback} is true:
                      cancel event
                    detect(player, "Scaffold", "2A")
                if {_dif.time} = 0 seconds:
                  add 0.5 to {vl.%player%}
                  add 1 to {GUARD.%player%.total.vl}
                  if {@setback} is true:
                    cancel event
                  detect(player, "Scaffold", "2A")
                  if {@Kick} is true:
                    make server execute command "/guard kick %player%"
                    stop
                  if {@Ban} is true:
                    make server execute command "/guard ban %player% 7 day"
            delete {GUARD.delay.scaffold.%player%}

