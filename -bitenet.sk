   wait 2 minutes
   broadcast "&2&lBiteNet &e&l>> &cHacking is &4&l&oNOT &callowed on &2&lBiteNet"
   wait 2 minutes
   broadcast "&2&lBiteNet &e&l>> &aWe are searching staff ! Apply on discord with /discord !"
   wait 2 minutes


#/// CLEARLAG ///

command /clearlag:
   permission: staff
   trigger:
      clear {_count}
      loop entities:
         loop-entity is a dropped item
         clear loop-entity
         add 1 to {_count}
      if {_count} is not set:
         set {_count} to 0
      if {_count} is not 0 or 1:
         broadcast "&2&lBiteNet &e&l>> %{_count}% items on the ground cleared !"
         make server execute command "/killall arrow"
      else:
         broadcast "&2&lBiteNet &e&l>> %{_count}% items on the ground cleared !"
         make server execute command "/killall arrow world"

every 590 seconds:
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 10 seconds"
   wait 5 seconds
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 5 seconds"
   wait 1 seconds
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 4 seconds"
   wait 1 seconds
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 3 seconds"
   wait 1 seconds
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 2 seconds"
   wait 1 seconds
   broadcast "&2&lBiteNet &e&l>> Clearing items on the ground in 1 seconds"
   wait 1 seconds
   make server execute command "/clearlag"

