#/// PING / ANTIPINGSPOOF ///

command /ping [<player>]:
  aliases: /p
  trigger:
    if arg 1 is not set:
      send "&ePing &6» &a%player's ping%"
    else:
      send "&ePing &6» &a%player-arg's ping%"

every 2 second:
   loop all players:
      if loop-player's ping is greater than 1000:
         send action bar "&e&lThe connection is unstable." to loop-player
         add 1 to {ping.%loop-player%}
         if {ping.%loop-player%} >= 10:
            kick loop-player due to "A communication error has occurred."

on join:
   clear {ping.%player%}

