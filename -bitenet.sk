command /debug:
   permission: debug.use
   trigger:
      if {debug.%player%} is not set:
         set {debug.%player%} to false
      if {debug.%player%} is false:
         set {debug.%player%} to true
         send "&2&lBiteNet &e&l>> &aYour now in debug mode"
         make server execute command "/sk disable debug"
         stop
      if {debug.%player%} is true:
         set {debug.%player%} to false
         send "&2&lBiteNet &e&l>> &cYou are no longer in debug mode"
         make server execute command "/sk enable debug"
         stop

command /ban [<offline player>] [<text>]:
   permission: ban.use
   trigger:
      if arg 1 is set:
         if arg 2 is not set:
            if arg 1 has permission "ban.bypass":
               send "&2&lBiteNet &e&l>> &cYou can't ban this person !" to player
               stop
            make server execute command "/smite %arg 1%"
            ban arg 1
            broadcast "&2&lBiteNet &e&l>> &c%player% &7has permanently banned &c%arg 1%"
            send "&2&lBiteNet &e&l>> &c%arg 1% &ahas been succecfuly banned!" to player
            add arg 1 to {banlist}
            stop
      if arg 1 is set:
         if arg 2 is set:
            if arg 1 has permission "ban.bypass":
               send "&2&lBiteNet &e&l>> &cYou can't ban this person !" to player
               stop
            make server execute command "/smite %arg 1%"
            ban arg 1 due to arg 2
            broadcast "&2&lBiteNet &e&l>> &c%player% &7has permanently banned &c%arg 1% &7for &6%arg 2%"
            send "&2&lBiteNet &e&l>> &c%arg 1% &ahas been succecfuly banned for &6%arg 2%" to player
            add arg 1 to {banlist}
            stop
      if arg 1 is not set:
         send "&2&lBiteNet &e&l>> &cUsage /ban PLAYER REASON" to player
         stop

command /unban [<text>]:
   permission: unban.use
   trigger:
      if arg 1 is set:
         make server execute command "/pardon %arg 1%"
         broadcast "&2&lBiteNet &e&l>> &e%player% &ahas unbanned &e%arg 1%"
         send "&2&lBiteNet &e&l>> &aYou succecfuly unbanned &e%arg 1%"
         stop
      if arg 1 is not set:
         send "&2&lBiteNet &e&l>> &cUsage: /unban PLAYER"
         stop

command /bans:
   permission: ban.list
   trigger:
      send "&2&lBiteNet &e&l>> &c%{banlist}%" to player
      send "&2&lBiteNet &e&l>> &c%{banlist}%" to console


command /alt [<offline player>]:
    permission: alt.see
    trigger:
        if arg 1 is not set:
            send "&cUsage : /alt <player>"
            stop
        if {ip.%arg 1%} is not set:
            send "&7[&2&lBiteNet&e&lAC&7] &cError: &4This player never joinned the server !"
            stop
        if {firstalt.%{ip.%arg 1%}%} is not set:
            send "&7[&2&lBiteNet&e&lAC&7] &cError: &4This player never joinned the server !"
            stop
        else:
            send "&7[&2&lBiteNet&e&lAC&7] &9Alts of %arg 1% : &c%{altip.%{ip.%arg 1%}%::*}%&9 First Account : &c%{firstalt.%{ip.%arg 1%}%}% &9Ip of the player : &c%{ip.%arg 1%}%"
on join:
    if {ip.%player%} is not set:
        remove player from {altip.%ip of player%::*}
        wait 1 tick
        add player to {altip.%ip of player%::*}
        set {ip.%player%} to ip of player
    if {firstalt.%ip of player%} is not set:
        remove player from {altip.%ip of player%::*}
        wait 1 tick
        add player to {altip.%ip of player%::*}
        set {firstalt.%ip of player%} to player's name
    else:
        remove "%player%" from {altip.%ip of player%::*}
        wait 1 tick
        add player to {altip.%ip of player%::*}
    if {firstalt.%ip of player%} is set:
        set {_name} to player's name
        if {_name} is {firstalt.%ip of player%}:
            stop
        else:
            loop all players:
                if loop-player has permission "alt.see":
                    send "&7[&2&lBiteNet&e&lAC&7] &c%{_name}% is an alt of %{firstalt.%ip of player%}%" to loop-player
            loop {altip.%ip of player%::*}:
                if loop-value is banned:
                    ban player due to "&cBan evading"
                    kick player due to "&cAlt of %loop-value%"
                    broadcast "&7[&2&lBiteNet&e&lAC&7] &9The play &c%player% &9has been banned for &cBan Evading '&cAlt of %loop-value%'"

command /suspend [<offline player>] [<text>]:
   permission: suspend.use
   trigger:
      if arg 1 is set:
         if arg 2 is set:
            if arg 1 has permission "ban.bypass":
